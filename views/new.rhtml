<% content_for :head do %>
<script src="/javascripts/jquery-1.8.2.min.js"></script>
<script src="/javascripts/config.js"></script>
<script src="/javascripts/GCAPI.js"></script>
<script src="/javascripts/games/<%= params[:name] %>.js"></script>
<script type="text/javascript">
  $(document).ready(function(){
    if (ensureConfigParameters()) {
      $('#game-title').text(game.title);
      $('#game-config').getConfigure(game.parameters);
    }
  });
</script>
<% end %>
<h1 id="game-title"></h1>
<div id="game-config">
</div>
